{
    "name": "meniam/text_generator",
    "type": "library",
    "description":  "Seo text generator tool",
    "keywords": ["app", "web", "seo", "text"],
    "homepage": "https://github.com/meniam/text_generator",
    "license":  "MIT",
    "authors": [
        {
            "name": "Julia Loykova",
            "email": "julinary@gmail.com"
        },
        {
            "name": "Eugene Myazin",
            "email": "meniam@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "meniam/permutation": ">=1.0.0"
    },
    "require-dev": {
        "php": ">=5.4.0"
    },
    "autoload": {
        "psr-0": {
            "TextGenerator": "library"
        }
    },
    "repositories":[
        {
            "type":"package",
            "package":{
                "name":"meniam/permutation",
                "version":"v1.0.0",
                "source":{
                    "type":"git",
                    "url":"git@github.com:meniam/permutation.git",
                    "reference":"master"
                },
                "autoload": {
                    "classmap" : ["src/"]
                }
            }
        }
    ]

}